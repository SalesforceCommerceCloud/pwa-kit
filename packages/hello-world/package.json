{
    "name": "hello-world",
    "version": "2.0.0-dev.2",
    "license": "See license in LICENSE",
    "private": true,
    "devDependencies": {
        "pwa-kit-cli": "^2.0.0-dev.2",
        "pwa-kit-runtime": "^2.0.0-dev.2"
    },
    "scripts": {
        "test": "pwa-kit-cli test",
        "format": "pwa-kit-cli format '\"**/*.{js,jsx}\"'",
        "lint": "pwa-kit-cli lint '\"**/*.{js,jsx}\"'",
        "lint:fix": "npm run lint -- --fix",
        "start": "pwa-kit-cli start",
        "build": "pwa-kit-cli build",
        "push": "npm run build && pwa-kit-cli push",
        "save-credentials": "pwa-kit-cli save-credentials"
    },
    "mobify": {
        "ssrEnabled": true,
        "ssrParameters": {
            "ssrFunctionNodeVersion": "14.x"
        },
        "ssrOnly": [
            "ssr.js",
            "node_modules/**/*.*"
        ],
        "ssrShared": [
            "intentionally-does-not-exist"
        ]
    }
}
