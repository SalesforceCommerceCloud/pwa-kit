{
    "name": "hello-world",
    "version": "2.0.0-dev.2",
    "license": "See license in LICENSE",
    "private": true,
    "devDependencies": {
        "sf-mrt": "^2.0.0-dev.2",
        "pwa-kit-runtime": "^2.0.0-dev.2"
    },
    "scripts": {
        "test": "sf-mrt test",
        "format": "sf-mrt format '\"**/*.{js,jsx}\"'",
        "lint": "sf-mrt lint '\"**/*.{js,jsx}\"'",
        "lint:fix": "npm run lint -- --fix",
        "start": "sf-mrt start",
        "build": "sf-mrt build",
        "push": "npm run build && sf-mrt push",
        "save-credentials": "sf-mrt save-credentials"
    },
    "mobify": {
        "ssrEnabled": true,
        "ssrParameters": {
            "ssrFunctionNodeVersion": "14.x"
        },
        "ssrOnly": [
            "ssr.js",
            "node_modules/**/*.*"
        ],
        "ssrShared": [
            "intentionally-does-not-exist"
        ]
    }
}
