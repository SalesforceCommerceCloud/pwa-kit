    "express-basic-auth": "^1.2.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "^16.11.0 || ^18.0.0",
    "npm": "^8.0.0 || ^9.0.0"
  },
  "mobify": {
    "ssrEnabled": true,
    "ssrParameters": {
      "ssrFunctionNodeVersion": "18.x",
      "proxyConfigs": [
        {
          "host": "httpbin.org",
