{
	"name": "template-mrt-reference-app",
	"version": "2.8.0-dev",
	"engines": {
		"node": "^16.0.0 || ^18.0.0",
		"npm": "^7.0.0 || ^8.0.0 || ^9.0.0"
	},
	"license": "See license in LICENSE",
	"private": true,
	"devDependencies": {
		"@loadable/component": "^5.15.0",
		"cross-fetch": "^3.1.4",
		"express-basic-auth": "^1.2.0",
		"supertest": "^4.0.2"
	},
	"mobify": {
		"ssrEnabled": true,
		"ssrParameters": {
			"ssrFunctionNodeVersion": "16.x",
			"proxyConfigs": [
				{
					"host": "httpbin.org",
					"path": "httpbin"
				}
			]
		},
		"ssrOnly": [
			"ssr.js",
			"node_modules/**/*.*"
		],
		"ssrShared": [
			"intentionally-does-not-exist"
		]
	}
}
