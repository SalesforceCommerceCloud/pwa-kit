  "scripts": {
    "analyze-build": "cross-env MOBIFY_ANALYZE=true npm run prod:build",
    "build-sprites": "npm run clean:svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-generator='pw-%s' --shape-id-separator=''  --dest=./app/static/svg/sprite-dist ./app/static/svg/sprite-source/*.svg",
    "clean:svgs": "svgo --enable='removeUselessStrokeAndFill' --disable='removeViewBox' -f ./app/static/svg/sprite-source",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:sass": "sass-lint -c .sass-lint.yml 'app/**/*.scss' -v",
    "push": "cross-env NODE_ENV=production npm run prod:build && sdk-upload",
    "save-credentials": "sdk-save-credentials",
    "start": "node ./scripts/start ssr",
    "start:inspect": "node ./scripts/start ssr --inspect",
    "prod:build": "cross-env NODE_ENV=production sdk-pwa-build",
    "test": "jest",
    "test:e2e": "nightwatch --suiteRetries 2 --config tests/e2e/nightwatch.conf.js tests/e2e/workflows/home-plp-pdp.js",
    "test:e2e-ci": "node ./scripts/run-e2e.js",
    "test:integration": "cross-env TEST_TYPE=integration npm run test",
    "test:max-file-size": "bundlesize",
    "format": "prettier --write \"**/*.{js,jsx}\""
  },
  "bundlesize": [
    {
      "path": "build/main.css",
      "maxSize": "10 kB"
    },
    {
      "path": "build/main.js",
      "maxSize": "15 kB"
    },
    {
      "path": "build/vendor.js",
      "maxSize": "215 kB"
    }
  ],
  "browserslist": [
    "iOS >= 9.0",
    "Android >= 4.4.4",
    "last 4 ChromeAndroid versions"
  ]
}
