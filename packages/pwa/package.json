      "ssr.js",
      "ssr.js.map",
      "static/js/browser-source-map-support.js",
      "static/js/setup-sourcemaps.js"
    ],
    "ssrShared": [
      "main.css",
      "static/ico/favicon.ico",
      "static/robots.txt",
      "**/*.js",
      "**/*.js.map"
    ],
    "ssrParameters": {
      "ssrFunctionNodeVersion": "12.x",
      "proxyConfigs": [
        {
          "protocol": "https",
          "host": "www.merlinspotions.com",
          "path": "base"
        },
        {
          "protocol": "https",
          "host": "zzrf-001.sandbox.us01.dx.commercecloud.salesforce.com",
          "path": "base2"
        }
      ]
    },
    "testFrameworkConfig": {
      "max_tti": 10000,
      "min_pwa_score": 90,
      "min_seo_score": 100,
      "min_accessibility_score": 100,
      "check_console_errors": true
    }
