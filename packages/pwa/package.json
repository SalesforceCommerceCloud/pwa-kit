  "config": {
    "sdk_generator_version": 3
  },
  "scripts": {
    "add:component": "sdk-generate component",
    "add:form": "sdk-generate form",
    "add:page": "node ./scripts/generator/generator.js --inputDir=./scripts/generator/assets/page-skeleton --outputDir=./app/pages",
    "analyze-build": "cross-env MOBIFY_ANALYZE=true npm run prod:build",
    "build-sprites": "npm run clean:svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-generator='pw-%s' --shape-id-separator='' --dest=./app/static/svg/sprite-dist ./app/static/svg/sprite-source/*.svg",
    "clean:svgs": "svgo --enable='removeUselessStrokeAndFill' --disable='removeTitle' -f ./app/static/svg/sprite-source",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:sass": "sass-lint -c .sass-lint.yml 'app/**/*.scss' -v",
    "push": "cross-env NODE_ENV=production npm run prod:build && sdk-upload",
    "save-credentials": "sdk-save-credentials",
    "start": "node ./scripts/start ssr",
    "ssr": "node ./scripts/start ssr",
    "start:ssr": "node ./scripts/start ssr",
    "start:ssr:inspect": "node ./scripts/start ssr --inspect",
    "prod:build": "rimraf build/ && sdk-create-hash-manifest --config cache-hash-config.json && cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --display-error-details --bail",
    "test": "jest --debug",
    "test:e2e-local": "mobify-test-framework nightwatch -- tests/e2e/workflows/home-plp-pdp.js",
    "test:e2e-ci": "node ./scripts/run-e2e.js",
    "test:lighthouse": "cross-env NODE_ENV=production ./scripts/run-lighthouse.js",
    "test:max-file-size": "bundlesize"
  },
  "bundlesize": [
    {
      "path": "build/main.css",
      "maxSize": "10 kB"
    },
    {
      "path": "build/main.js",
      "maxSize": "15 kB"
    },
    {
      "path": "build/vendor.js",
      "maxSize": "215 kB"
    }
  ],
  "browserslist": [
    "iOS >= 9.0",
    "Android >= 4.4.4",
    "last 4 ChromeAndroid versions"
  ]
}
