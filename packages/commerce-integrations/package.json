{
    "name": "@mobify/commerce-integrations",
    "version": "1.20.5",
    "description": "A customizable API abstraction layer for ecommerce backends.",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rimraf dist && babel src --ignore '*.test.js','test.js','tapes/*' --out-dir dist --quiet",
        "jsdoc": "node ./scripts/jsdoc.js",
        "docs": "rimraf docs && npm run jsdoc",
        "deployDocs": "node ./scripts/deployDocs.js",
        "prepare": "npm run build",
        "version": "node ./scripts/version.js",
        "lint": "npm run lint:js",
        "lint:fix": "npm run lint:js -- --fix",
        "lint:js": "eslint \"**/*.{js,jsx}\"",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mobify/commerce-integrations.git"
    },
    "author": "dev@mobify.com",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/mobify/commerce-integrations/issues"
    },
    "homepage": "https://github.com/mobify/commerce-integrations#readme",
    "dependencies": {
        "btoa": "^1.2.1",
        "commercecloud-ocapi-client": "^0.2.0",
        "fast-json-stable-stringify": "^2.1.0",
        "fs-extra": "^7.0.1",
        "hybris-occ-client": "^0.1.5",
        "i18n-iso-countries": "^3.6.3",
        "prop-types": "^15.6.1",
        "superagent": "^3.8.3",
        "url-parse": "^1.4.7",
        "validator": "^10.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@mobify/documentation-theme": "^1.20.5",
        "babel-jest": "^24.8.0",
        "cpr": "^3.0.1",
        "cross-spawn": "^6.0.5",
        "eslint": "^5.0.0",
        "fs-extra": "^7.0.1",
        "immutable": "^3.8.2",
        "jest-cli": "^24.9.0",
        "jsdoc": "^3.6.3",
        "jsdom": "^11.6.2",
        "prop-types": "^15.6.1",
        "react": "^16.8.6",
        "redux": "^3.7.2",
        "redux-actions": "^2.3.0",
        "reselect": "^3.0.1",
        "rimraf": "^2.6.2"
    },
    "harp": {
        "processors": {
            "md": {
                "smartypants": true
            }
        }
    }
}
