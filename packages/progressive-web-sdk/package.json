{
  "name": "progressive-web-sdk",
  "version": "1.20.4",
  "description": "A set of libraries and components which make up the SDK for Progressive Web",
  "main": "dist/index.js",
  "bin": {
    "sdk-upload": "bin/upload.js",
    "mobify": "bin/mobify.js",
    "sdk-messaging": "bin/messaging.js",
    "sdk-generate": "bin/generate.js",
    "sdk-save-credentials": "bin/save-credentials.js",
    "sdk-create-hash-manifest": "bin/create-hash-manifest.js",
    "sdk-get-routes": "bin/get-routes.js"
  },
  "engines": {
    "node": "^10.17.0 || ^12.0.0"
  },
  "files": [
    "bin",
    "generator-assets",
    "dist",
    "LICENSE",
    "src",
    "scripts",
    "vendor"
  ],
  "scripts": {
    "add:component": "node bin/generate.js component",
    "build-sprites": "npm run clean-svgs && svg-sprite --symbol --symbol-dest='.' --symbol-sprite='sprite.svg' --shape-id-generator='pw-%s' --dest=./styleguide/svg/sprite-dist ./styleguide/svg/sprite-source/*.svg",
    "clean-svgs": "node_modules/.bin/svgo --enable='removeUselessStrokeAndFill' -f ./styleguide/svg/sprite-source",
    "copyright:lint": "copyright \"src/*.js?(x)\" \"src/!(static)/**/*.js?(x)\" \"bin/**/*.js?(x)\" \"tests/**/*.js?(x)\" \"scripts/**/*.js?(x)\"",
    "copyright:fix": "npm run copyright:lint -- --fix --update ",
    "prepare": "npm run prod:build",
    "prod:build": "cross-env NODE_ENV=production scripts/build.js",
    "dev:build": "echo \"This command is deprecated. Please use 'npm run prod:build'.\"; sleep 5; npm run prod:build",
    "dev:watch": "watch 'npm run dev:build' src/",
    "lint": "npm run lint:js && npm run lint:sass && npm run copyright:lint",
    "lint:fix": "npm run lint:js -- --fix && npm run copyright:fix",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:sass": "sass-lint -c node_modules/mobify-code-style/css/.sass-lint.yml 'src/**/*.scss' -v",
    "test": "cross-env NODE_ENV=test jest --runInBand --coverage",
    "test:watch": "npm test -- --watch",
    "test:inspect": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "version": "node ./scripts/version.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobify/progressive-web-sdk.git"
  },
  "author": "dev@mobify.com",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/mobify/progressive-web-sdk/issues"
  },
  "homepage": "https://github.com/mobify/progressive-web-sdk#readme",
  "dependencies": {
    "@babel/parser": "^7.5.5",
    "@babel/runtime-corejs2": "^7.4.5",
    "@babel/traverse": "^7.5.5",
    "@types/request-promise": "4.1.42",
    "archiver": "1.3.0",
    "aws-sdk": "^2.814.0",
    "aws-serverless-express": "3.3.5",
    "babel-runtime": "6.23.0",
    "bluebird": "3.5.1",
    "bufferutil": "4.0.0",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "compression": "1.7.4",
    "cross-spawn": "^6.0.5",
    "debounce": "^1.2.0",
    "es6-promise": "4.1.0",
    "express": "4.16.4",
    "file-url": "^2.0.2",
    "git-rev-sync": "1.8.0",
    "glob": "7.1.1",
    "header-case": "1.0.1",
    "http-proxy-middleware": "0.19.0",
    "inline-style-prefixer": "2.0.5",
    "javascript-stringify": "1.6.0",
    "jquery": "^3.5.0",
    "js-yaml": "^3.13.1",
    "jsdom": "^14.0.0",
    "lodash": "^4.17.11",
    "lodash.escaperegexp": "4.1.2",
    "lodash.frompairs": "4.0.1",
    "lodash.intersection": "4.4.0",
    "lodash.map": "4.6.0",
    "lodash.mapvalues": "4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.throttle": "4.1.1",
    "loglevel": "1.6.3",
    "mime-types": "2.1.20",
    "minimatch": "3.0.4",
    "mkdirp": "^0.5.2",
    "morgan": "1.9.1",
    "node-fetch": "^2.6.1",
    "postcss-loader": "^4.0.0",
    "prompt": "1.0.0",
    "prop-types": "^15.6.0",
    "query-string": "4.3.2",
    "raf": "^3.4.0",
    "react-group": "1.0.5",
    "react-input-mask": "1.0.7",
    "react-onclickoutside": "^6.7.1",
    "react-router": "3.2.0",
    "react-router-scroll": "0.4.4",
    "react-transition-group": "^1.0.0",
    "react-uid": "^2.2.0",
    "request": "^2.88.0",
    "request-promise": "4.2.2",
    "rimraf": "2.6.1",
    "semver": "5.6.0",
    "set-cookie-parser": "^2.2.1",
    "source-map-support": "0.5.9",
    "store": "2.0.4",
    "sw-toolbox": "3.6.0",
    "tti-polyfill": "0.2.2",
    "ua-parser-js": "^0.7.20",
    "uncss": "^0.16.2",
    "utf-8-validate": "5.0.1",
    "validator": "10.2.0",
    "whatwg-encoding": "^1.0.5",
    "yargs": "6.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@mobify/documentation-theme": "^1.20.4",
    "autoprefixer": "6.7.7",
    "aws-lambda-test-utils": "^1.3.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "copy": "0.3.2",
    "cross-env": "3.2.3",
    "css-loader": "^1.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "eslint": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "^2.1.1",
    "immutable": "3.8.1",
    "jest-cli": "^24.9.0",
    "jest-date-mock": "^1.0.7",
    "jest-each": "^24.8.0",
    "jest-environment-jsdom": "^24.8.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-expect-message": "1.0.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "jsdoc": "^3.5.5",
    "mobify-code-style": "^2.8.4",
    "mobify-debug-js": "0.0.1",
    "nock": "10.0.2",
    "node-sass": "^4.11.0",
    "normalize.css": "4.2.0",
    "npm-run-all": "3.1.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "5.0.6",
    "redux": "3.6.0",
    "redux-actions": "2.0.1",
    "redux-form": "7.2.1",
    "redux-mock-store": "1.2.3",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.0",
    "reselect-immutable-helpers": "1.2.2",
    "runtypes": "0.12.0",
    "s3rver": "^3.1.0",
    "sandy-tracking-pixel-client": "0.1.31",
    "sass-lint": "^1.13.1",
    "sass-loader": "6.0.5",
    "service-worker-mock": "1.9.2",
    "sinon": "2.3.2",
    "susy": "3.0.5",
    "svg-sprite": "^1.5.0",
    "sw-test-env": "^2.1.0",
    "text-loader": "0.0.1",
    "watch": "1.0.1",
    "webpack": "2.6.1"
  },
  "peerDependencies": {
    "immutable": "3",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "react-redux": "5",
    "react-transition-group": "1",
    "redux": "3",
    "redux-actions": "1 || 2",
    "redux-form": "7",
    "reselect": "2 || 3",
    "reselect-immutable-helpers": "1",
    "runtypes": "0.12"
  },
  "bundledDependencies": [
    "jsdom"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(jsdom/lib/jsdom/browser/resources/resource-loader))"
    ],
    "setupFiles": [
      "<rootDir>/scripts/setup-jest.js",
      "jest-localstorage-mock",
      "jest-fetch-mock",
      "jest-date-mock"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "cacheDirectory": "./node_modules/.cache",
    "clearMocks": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/test.{js,jsx}",
      "!generator-assets/**/*.{js,jsx}",
      "!coverage/**/*.js",
      "!dist/**/*.{js,jsx}",
      "!docs/**/*.{js,jsx}",
      "!styleguide/**/*.{js,jsx}",
      "!vendor/**/*",
      "!*.config.js",
      "!src/index.js",
      "!src/{console-error-reporter,polyfill,test-utils,load-scripts}.js",
      "!src/routing/index.jsx",
      "!src/integration-manager/index.js",
      "!src/integration-manager/types.js",
      "!src/integration-manager/results.js",
      "!src/integration-manager/commands.js",
      "!src/integration-manager/reducer.js",
      "!src/integration-manager/*/{commands,results,types}.js",
      "!bin/*.js",
      "!src/components/{form-fields,nested-navigation,select}/*.{js,jsx}",
      "!src/patterns/**/*.{js,jsx}",
      "!src/templates/**/*.{js,jsx}",
      "!src/ssr/ssr-polyfills.js",
      "!src/ssr/test_fixtures/*.{js,jsx}",
      "!src/worker/{main,test-entrypoint}.js",
      "!temp/**/*",
      "!temp_static/**/*",
      "!src/static/*",
      "!tests/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 97,
        "functions": 91,
        "lines": 94,
        "statements": 94
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.svg$": "<rootDir>/tests/emptyStringMock.js"
    },
    "testPathIgnorePatterns": [
      "/dist/",
      "/temp/",
      "/generator-assets/",
      "/node_modules/",
      "/vendor/"
    ],
    "testRegex": "\\.?(test|spec)\\.jsx?$",
    "testEnvironment": "jest-environment-jsdom-global",
    "testEnvironmentOptions": {
      "resources": "usable"
    }
  },
  "harp": {
    "processors": {
      "md": {
        "smartypants": true
      }
    }
  }
}
