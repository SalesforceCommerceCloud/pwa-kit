{
    "name": "template-express-minimal",
    "version": "2.0.0-dev.5",
    "license": "See license in LICENSE",
    "private": true,
    "devDependencies": {
        "babel-preset-pwa-kit": "^2.0.0-dev.5",
        "eslint": "7.31.0",
        "eslint-config-pwa-kit": "^2.0.0-dev.5",
        "pwa-kit-cli": "^2.0.0-dev.5",
        "pwa-kit-runtime": "^2.0.0-dev.5"
    },
    "scripts": {
        "test": "pwa-kit-cli test",
        "format": "pwa-kit-cli format \"**/*.{js,jsx}\"",
        "lint": "eslint \"**/*.{js,jsx}\"",
        "lint:fix": "npm run lint -- --fix",
        "start": "pwa-kit-cli start",
        "build": "pwa-kit-cli build",
        "push": "npm run build && pwa-kit-cli push",
        "save-credentials": "pwa-kit-cli save-credentials"
    },
    "mobify": {
        "ssrEnabled": true,
        "ssrParameters": {
            "ssrFunctionNodeVersion": "14.x"
        },
        "ssrOnly": [
            "ssr.js",
            "node_modules/**/*.*"
        ],
        "ssrShared": [
            "intentionally-does-not-exist"
        ]
    }
}
