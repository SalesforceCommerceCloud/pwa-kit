{
    "name": "template-express-minimal",
    "version": "2.0.0-dev.3",
    "license": "See license in LICENSE",
    "private": true,
    "devDependencies": {
        "pwa-kit-build": "^2.0.0-dev.3",
        "pwa-kit-runtime": "^2.0.0-dev.3"
    },
    "scripts": {
        "test": "mrt test",
        "format": "mrt format '\"**/*.{js,jsx}\"'",
        "lint": "mrt lint '\"**/*.{js,jsx}\"'",
        "lint:fix": "npm run lint -- --fix",
        "start": "mrt start",
        "build": "mrt build",
        "push": "npm run build && mrt push",
        "save-credentials": "mrt save-credentials"
    },
    "mobify": {
        "ssrEnabled": true,
        "ssrParameters": {
            "ssrFunctionNodeVersion": "14.x"
        },
        "ssrOnly": [
            "ssr.js",
            "node_modules/**/*.*"
        ],
        "ssrShared": [
            "intentionally-does-not-exist"
        ]
    }
}
