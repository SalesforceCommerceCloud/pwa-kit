                  - ApiStandards.RateLimited:
                      responseStatus: 404
                  - CustomerTraits.NotFound
                responses:
                  "204":
                    description: No Content
                securedBy:
                  - CommerceCloudStandards.ShopperToken: { scopes: [sfcc.shopper-myaccount.productlists.rw] }
              get:
